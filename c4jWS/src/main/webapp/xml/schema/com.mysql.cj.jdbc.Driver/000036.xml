<statement>
ALTER TABLE `APP_PALLET` ADD COLUMN `MHN_NUMBER` VARCHAR(10) AFTER `CUSTOMER_ID`, ADD COLUMN `DECISION` VARCHAR(10) AFTER `MHN_NUMBER`;
</statement>

<statement>
ALTER TABLE `APP_PALLET` ADD INDEX `PALLET_MHN_IDX`(`MHN_NUMBER`);
</statement>

<statement>
ALTER TABLE `APP_PALLET_HISTORY` ADD COLUMN `MHN_NUMBER` VARCHAR(10) AFTER `CUSTOMER_ID`, ADD COLUMN `DECISION` VARCHAR(10) AFTER `MHN_NUMBER`;
</statement>

<statement>
CREATE TABLE `APP_MHN` (
  `MHN_NUMBER` VARCHAR(10) NOT NULL,
  `INITIATOR` VARCHAR(20),
  `RECORDER` VARCHAR(20),
  `AUTHORISOR` VARCHAR(20),
  `REASON1` VARCHAR(10),
  `REASON2` VARCHAR(10),
  `REASON3` VARCHAR(10),
  `DATE_CREATED` DATETIME,
  `DATE_EXPECTED` DATETIME,
  `DATE_RESOLVED` DATETIME,
  `STATUS` VARCHAR(20),
  `REQUIRED_RESOURCE` VARCHAR(50),
  `COMMENT` VARCHAR(250),
  PRIMARY KEY (`MHN_NUMBER`)
)
ENGINE = InnoDB;
</statement>

<statement>
CREATE TABLE `APP_MHN_DECISIONS` (
  `DECISION` VARCHAR(10) NOT NULL,
  `DESCRIPTION` VARCHAR(50),
  `COLOR_FOREGROUND` INTEGER,
  `COLOR_BACKGROUND` INTEGER,
   PRIMARY KEY (`DECISION`)
)
 ENGINE=InnoDB;
</statement>

<statement>
CREATE TABLE `APP_MHN_REASONS` (
  `REASON` VARCHAR(10) NOT NULL,
  `DESCRIPTION` VARCHAR(50),
   PRIMARY KEY (`REASON`)
)
 ENGINE=InnoDB;
</statement>

