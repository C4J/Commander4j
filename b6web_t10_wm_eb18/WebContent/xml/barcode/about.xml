<?xml version="1.0"?>
<!DOCTYPE barcode SYSTEM "barcode4j.dtd">
<barcode>
  <ean-128>
      <height>8mm</height>
      <module-width>0.1mm</module-width>
      <check-digit-marker>&#xF0;</check-digit-marker>
      <group-separator>&#xF1;</group-separator>
      <template>(400)an40</template>
      <quiet-zone enabled="false">0mm</quiet-zone>
      <human-readable>
          <placement>none</placement>
          <font-name>Helvetica</font-name>
          <font-size>8pt</font-size>
          <omit-brackets>false</omit-brackets>
      </human-readable>
  </ean-128>
</barcode>