	<queryString language="SQL">
		<![CDATA[SELECT
	LEFT(PACK_PANEL , 3) AS IN_OUT,
	COUNT(LEFT(PRODUCT_PANEL , 3)) AS COUNTER,
	(SELECT COUNT(LEFT(PACK_PANEL , 3)) FROM view_qm_analysis_01 WHERE BATCH_SUFFIX = $P{P_BATCH_SUFFIX} AND 	SAMPLE_DATE >= $P{P_SAMPLE_DATE_START}  AND 	SAMPLE_DATE <= $P{P_SAMPLE_DATE_END} ) AS TOTAL
FROM
	view_qm_analysis_01
WHERE
	BATCH_SUFFIX =$P{P_BATCH_SUFFIX}
AND 	SAMPLE_DATE >= $P{P_SAMPLE_DATE_START} 
AND 	SAMPLE_DATE <= $P{P_SAMPLE_DATE_END} 
GROUP BY
	BATCH_SUFFIX ,
	LEFT(PACK_PANEL , 3)]]>
	</queryString>
